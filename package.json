{
    "name": "strype-editor",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve -- --port 8081",
        "serve:microbit": "cross-env npm_config_microbit=true vue-cli-service serve --port 8081",
        "serve:python": "cross-env npm_config_python=true vue-cli-service serve --port 8081",
        "build": "vue-cli-service build",
        "test:unit": "vue-cli-service test:unit --require test-setup.js --require test-setup-python.js src/__tests__/*.ts && vue-cli-service test:unit --require test-setup.js --require test-setup-microbit.js src/__tests__/*.ts",
        "lint:check": "vue-cli-service lint --no-fix --max-warnings 0",
        "lint:fix": "vue-cli-service lint",
        "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/localisation/**/*.json'",
        "serve:https": "HTTPS=true vue-cli-service serve",
        "cy:run:microbit": "cypress run --env mode=microbit",
        "cy:run:python": "cypress run --env mode=python",
        "test:cypress": "cross-env WAIT_ON_TIMEOUT=60000 start-server-and-test serve:python http://localhost:8081/editor/ cy:run:python && cross-env WAIT_ON_TIMEOUT=60000 start-server-and-test serve:microbit http://localhost:8081/microbit/ cy:run:microbit"
    },
    "dependencies": {
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "@microbit/microbit-fs": "^0.9.2",
        "@popperjs/core": "^2.11.4",
        "@types/jquery": "^3.5.14",
        "@types/lodash": "^4.14.181",
        "@types/object-hash": "^1.3.3",
        "@types/sortablejs": "^1.10.6",
        "@types/vuedraggable": "^2.23.2",
        "@vue/composition-api": "^1.4.9",
        "axios": "^0.21.4",
        "bootstrap": "^4.6.1",
        "bootstrap-vue": "^2.16.0",
        "cheerio": "^1.0.0-rc.5",
        "core-js": "^3.21.1",
        "dapjs": "^2.2.0",
        "file-saver": "^2.0.2",
        "jquery": "^3.6.0",
        "lodash": "^4.17.20",
        "lz-string": "^1.4.4",
        "move-assets-webpack-plugin": "^1.3.2",
        "object-hash": "^2.0.3",
        "pinia": "^2.0.13",
        "python-shell": "^2.0.3",
        "sortablejs": "^1.15.0",
        "textarea-caret": "^3.1.0",
        "tigerpython-parser": "git+https://gitlab.bluej.org/webframes/tiger-python.git",
        "v-blur": "^1.0.4",
        "velocity": "^0.7.3",
        "vue": "^2.6.12",
        "vue-autosize-input": "^0.1.1",
        "vue-browser-detect-plugin": "^0.1.18",
        "vue-burger-menu": "^2.0.5",
        "vue-confirm-dialog": "^1.0.2",
        "vue-i18n": "^8.27.1",
        "vue-input-autowidth": "^1.0.10",
        "vue-simple-context-menu": "^3.4.2",
        "vuedraggable": "^2.24.3"
    },
    "devDependencies": {
        "@pinia/testing": "^0.0.14",
        "@types/chai": "^4.2.11",
        "@types/file-saver": "^2.0.5",
        "@types/lz-string": "^1.3.34",
        "@types/mocha": "^5.2.4",
        "@types/textarea-caret": "^3.0.0",
        "@types/w3c-web-usb": "^1.0.6",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "@vue/cli-plugin-babel": "^4.5.17",
        "@vue/cli-plugin-eslint": "^4.5.17",
        "@vue/cli-plugin-typescript": "^4.5.17",
        "@vue/cli-plugin-unit-mocha": "~4.5.15",
        "@vue/cli-plugin-vuex": "^4.5.17",
        "@vue/cli-service": "^4.5.17",
        "@vue/eslint-config-typescript": "^7.0.0",
        "@vue/test-utils": "^1.3.0",
        "arkit": "^1.6.4",
        "chai": "^4.1.2",
        "cross-env": "^7.0.3",
        "cypress": "^10.9.0",
        "eslint": "^6.0.0",
        "eslint-plugin-vue": "^7.20.0",
        "husky": "^7.0.4",
        "jest": "^27.0.5",
        "jest-transform-stub": "^2.0.0",
        "js-conditional-compile-loader": "^1.0.15",
        "lint-staged": "^11.2.6",
        "mocha": "^10.0.0",
        "mochapack": "^2.1.4",
        "null-loader": "^4.0.1",
        "sass": "^1.49.9",
        "sass-loader": "^10.2.1",
        "start-server-and-test": "^1.14.0",
        "typescript": "^4.6.3",
        "vue-cli-plugin-i18n": "2.1.1",
        "vue-template-compiler": "^2.6.12",
        "webpack": "^4.0.0",
        "webpack-cli": "^4.9.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{ts,vue}": [
            "npm run lint"
        ]
    }
}
